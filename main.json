[
  {
    "match": "(\\.|^)topito\\.com$",
    "style": {
      "display": {
        "#didomi-popup" : "none"
      },
      "overflow" : {
        "body.didomi-popup-open" : "unset"
      },
      "position": {
        "body.didomi-popup-open" : "inherit"
      }
    }
  },
  {
    "match": "(\\.|^)instagram\\.com$",
    "style": {
      "display": {
        "div[role=\"presentation\"]" : "none"
      },
      "overflow" : {
        "body" : "unset"
      }
    }
  }
]